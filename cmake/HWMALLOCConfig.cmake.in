@PACKAGE_INIT@
set(HWMALLOC_MODULE_PATH ${CMAKE_CURRENT_LIST_DIR})
list(APPEND CMAKE_MODULE_PATH ${HWMALLOC_MODULE_PATH})
include(CMakeFindDependencyMacro)
if (@HWMALLOC_WITH_MPI@)
    find_dependency(MPI)
else()
endif()
if (@HWMALLOC_WITH_UCX@)
    find_dependency(UCP)
else()
endif()
if (@HWMALLOC_WITH_LIBFABRIC@)
    find_dependency(Libfabric)
else()
endif()
include(${CMAKE_CURRENT_LIST_DIR}/HWMALLOC-targets.cmake)
